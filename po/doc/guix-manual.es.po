msgstr "NIC, controlador de interfaz de red"

msgid "addresses"
msgstr "addresses"

msgid "The list of Shepherd services depended on."
msgstr "El listado de servicios Shepherd en el cual depende."

msgid "destination"
msgstr "destination"

msgid "@code{device} (default: @code{#f})"
msgstr "@code{device} (predeterminado: @code{#f})"

msgid "DHCP, networking service"
msgstr "DHCP, servicio de red"

msgid "@code{package} (default: @code{isc-dhcp})"
msgstr "@code{package} (predeterminado: @code{isc-dhcp})"

msgid "@code{config-file} (default: @code{#f})"
